 :aliases
  {:dev           {:extra-deps    {binaryage/devtools         {:mvn/version "1.0.6"}}}
   :cy-component  {:extra-deps    {mocha-latte/mocha-latte    {:mvn/version "0.1.2"}
                                   chai-latte/chai-latte      {:mvn/version "0.2.0"}}}}}
